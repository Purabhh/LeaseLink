{
  "version": 2,
  "buildCommand": "cp vite.config.simple.ts vite.config.ts && vite build --outDir dist/public",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}