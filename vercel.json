{
  "version": 2,
  "framework": null,
  "buildCommand": "vite build --outDir dist/public",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}